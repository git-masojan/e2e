  "description": "API and WEb Automation",
  "main": "index.js",
  "scripts": {
    "clean-reports": "cypress/reports/results/*.json cypress/reports/features cypress/reports/index.html",
    "test-webui": "npm run ./node_modules/.bin/cypress-tags run --spec \"cypress/integration/*.feature\" --record --key 123-456-4c8b-9b7b-1234567890",
